{
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@turbo/gen": "2.5.4",
    "@vitest/coverage-v8": "3.2.3",
    "@vitest/ui": "3.2.3",
    "cspell": "9.1.1",
    "editorconfig-checker": "6.0.1",
    "lefthook": "1.11.14",
    "syncpack": "13.0.4",
    "turbo": "2.5.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.6.1"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "clean": "turbo clean && rm -rf .turbo node_modules",
    "coverage": "vitest run --coverage",
    "dev": "turbo dev watch",
    "editorconfig-checker": "EC_VERSION=v3.2.0 editorconfig-checker",
    "lint": "biome check --error-on-warnings",
    "preinstall": "npx only-allow pnpm",
    "prepare": "lefthook install",
    "syncpack-fix-mismatches": "syncpack fix-mismatches",
    "syncpack-format": "syncpack format",
    "syncpack-lint": "syncpack lint",
    "synpack-update": "syncpack update",
    "test": "vitest",
    "ui": "vitest --ui --watch"
  }
}
