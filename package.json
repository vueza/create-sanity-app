{
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@turbo/gen": "2.5.3",
    "@vitest/coverage-v8": "3.1.3",
    "@vitest/ui": "3.1.3",
    "cspell": "9.0.0",
    "editorconfig-checker": "6.0.1",
    "lefthook": "1.11.12",
    "syncpack": "13.0.4",
    "turbo": "2.5.3",
    "vitest": "3.1.3"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.6.1"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "clean": "turbo clean && rm -rf .turbo node_modules",
    "coverage": "vitest run --coverage",
    "dev": "turbo dev watch",
    "editorconfig-checker": "EC_VERSION=v3.2.0 editorconfig-checker",
    "lint": "biome check --error-on-warnings",
    "preinstall": "npx only-allow pnpm",
    "prepare": "lefthook install",
    "syncpack-fix-mismatches": "syncpack fix-mismatches",
    "syncpack-format": "syncpack format",
    "syncpack-lint": "syncpack lint",
    "synpack-update": "syncpack update",
    "test": "vitest",
    "ui": "vitest --ui --watch"
  }
}
