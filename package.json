{
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@turbo/gen": "2.5.6",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cspell": "9.2.0",
    "editorconfig-checker": "6.1.0",
    "lefthook": "1.12.3",
    "syncpack": "13.0.4",
    "turbo": "2.5.6",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.14.0"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "clean": "turbo clean && rm -rf .turbo node_modules",
    "coverage": "vitest run --coverage",
    "dev": "turbo dev watch",
    "editorconfig-checker": "EC_VERSION=v3.2.0 editorconfig-checker",
    "lint": "biome check --error-on-warnings",
    "preinstall": "npx only-allow pnpm",
    "prepare": "lefthook install",
    "syncpack-fix-mismatches": "syncpack fix-mismatches",
    "syncpack-format": "syncpack format",
    "syncpack-lint": "syncpack lint",
    "synpack-update": "syncpack update",
    "test": "vitest",
    "ui": "vitest --ui --watch"
  }
}
