{
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@turbo/gen": "2.5.0",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": "3.1.1",
    "cspell": "8.18.1",
    "editorconfig-checker": "6.0.1",
    "lefthook": "1.11.8",
    "syncpack": "13.0.3",
    "turbo": "2.5.0",
    "vitest": "3.1.1"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.6.1"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "clean": "turbo clean && rm -rf .turbo node_modules",
    "coverage": "vitest run --coverage",
    "dev": "turbo dev watch",
    "editorconfig-checker": "EC_VERSION=v3.2.0 editorconfig-checker",
    "lint": "biome check --error-on-warnings",
    "preinstall": "npx only-allow pnpm",
    "prepare": "lefthook install",
    "syncpack-fix-mismatches": "syncpack fix-mismatches",
    "syncpack-format": "syncpack format",
    "syncpack-lint": "syncpack lint",
    "synpack-update": "syncpack update",
    "test": "vitest",
    "ui": "vitest --ui --watch"
  }
}
