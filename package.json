{
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@turbo/gen": "2.5.8",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cspell": "9.2.2",
    "editorconfig-checker": "6.1.1",
    "lefthook": "2.0.0",
    "syncpack": "13.0.4",
    "turbo": "2.5.8",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.14.0"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
  "scripts": {
    "build": "turbo build",
    "check-types": "turbo check-types",
    "clean": "turbo clean && rm -rf .turbo node_modules",
    "coverage": "vitest run --coverage",
    "dev": "turbo dev watch",
    "editorconfig-checker": "EC_VERSION=v3.2.0 editorconfig-checker",
    "lint": "biome check --error-on-warnings",
    "preinstall": "npx only-allow pnpm",
    "prepare": "lefthook install",
    "syncpack-fix-mismatches": "syncpack fix-mismatches",
    "syncpack-format": "syncpack format",
    "syncpack-lint": "syncpack lint",
    "synpack-update": "syncpack update",
    "test": "vitest",
    "ui": "vitest --ui --watch"
  }
}
