{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["SANITY_STUDIO_PROJECT_ID", "SANITY_STUDIO_DATASET"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "storybook-static/**",
        "dist/**",
        ".sanity/**"
      ]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "coverage": {
      "dependsOn": ["^clean"],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "watch": {
      "cache": false,
      "persistent": true
    }
  }
}
